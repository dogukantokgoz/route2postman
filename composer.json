{
    "name": "dogukantokgoz/route-to-postman",
    "description": "Generate Postman collections directly from Laravel routes with structured grouping and documentation.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "symfony/console": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "dogukantokgoz\\Route2Postman\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dogukan Tokgoz",
            "email": "dogukantokgoz4@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "dogukantokgoz\\Route2Postman\\PostmanServiceProvider"
            ],
            "logo": "https://imgur.com/a/route2postman-1EmSwbm"
        }
    },
    "license": "MIT"
}